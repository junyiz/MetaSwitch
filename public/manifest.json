{
    "name": "MetaSwitch",
    "description": "快捷地管理和切换多个代理设置",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "proxy"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/icon16.png",
        "32": "/images/icon32.png",
        "48": "/images/icon48.png",
        "128": "/images/icon48.png"
      }
    },
    "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon48.png"
    },
    "options_page": "index.html",
    "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>", "file:///*", "*://*/*" ],
      "resources": [
        "main.js",
        "main.css",
        "lib/monaco-editor/vs/loader.js",
        "lib/monaco-editor/vs/editor/editor.main.js",
        "lib/monaco-editor/vs/editor/editor.main.css",
        "lib/monaco-editor/vs/editor/editor.main.nls.js",
        "lib/monaco-editor/vs/language/json/jsonMode.js",
        "lib/monaco-editor/vs/language/json/jsonWorker.js",
        "lib/monaco-editor/vs/base/worker/workerMain.js",
        "lib/monaco-editor/vs/base/common/worker/simpleWorker.nls.js",
        "lib/monaco-editor/vs/base/browser/ui/codicons/codicon/codicon.ttf"
      ]
   } ]
}